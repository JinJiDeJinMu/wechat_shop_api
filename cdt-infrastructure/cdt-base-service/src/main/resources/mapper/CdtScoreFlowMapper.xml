<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chundengtai.base.dao.CdtScoreFlowMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.chundengtai.base.bean.CdtScoreFlow">
        <id column="id" property="id"/>
        <result column="user_id" property="userId"/>
        <result column="flow_sn" property="flowSn"/>
        <result column="score_id" property="scoreId"/>
        <result column="score_sum" property="scoreSum"/>
        <result column="money" property="money"/>
        <result column="offset_money" property="offsetMoney"/>
        <result column="pay_status" property="payStatus"/>
        <result column="create_time" property="createTime"/>
        <result column="pay_time" property="payTime"/>
        <result column="token" property="token"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, flow_sn, score_id, score_sum, money, offset_money, pay_status, create_time, pay_time, token
    </sql>

</mapper>
